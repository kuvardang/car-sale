otp-fade');}else{$('#otpSubmitDiv').addClass('ty-noneActive').addClass('otp-fade');$('#otpDiv').addClass('otp-fade');}}function verifyMobileViaAPI(){var mobile=jQuery('#otp_mobileNoToBeVerified').val();}</script><!--googleoff: all-->
